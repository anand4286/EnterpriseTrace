# Enterprise Trace npm Configuration
# This file ensures consistent npm behavior across all machines

# Enforce engine version checking
engine-strict=true

# Security settings
audit-level=moderate
fund=false

# Performance settings
prefer-offline=true
progress=true

# Registry settings (uncomment if using private registry)
# registry=https://your-private-registry.com

# Cache settings
cache-min=10
cache-max=60

# Package installation settings
package-lock=true
save-exact=false
save-prefix=^

# Logging
loglevel=warn
